{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "object-curly-newline": ["error", { "multiline": true }],
    "implicit-arrow-linebreak": "off",
    "comma-dangle": "off",
    "quotes": "off",
    "linebreak-style": "off",
    "no-console": "off"
  }
}
